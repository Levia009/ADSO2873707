Proceso ejercicio_XVlll
	
	Definir capital,ganancias,men,anu,final Como Real;
	
	Escribir "cuanta capital desea invertir";
	Leer capital;
	
	men<-2.5;
	anu<-(men*12);
	ganancias<-(anu*capital/100);
	final<-(capital+ganancias);
	
	Escribir "el dinero que ganara en el plaso de un año sera ", final," pesos";
	
FinProceso
