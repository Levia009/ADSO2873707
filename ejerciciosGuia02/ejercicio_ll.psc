Proceso ejercicio_ll
	
	Definir total,porcentaje1,porcentaje2,hombres,mujeres Como real;
	
	Escribir "cuantas mujeres hay en su grupo";
	Leer mujeres;
	Escribir "cuantos hombres hay en su grupo";
	Leer hombres;
	
	total<-hombres+mujeres;
	porcentaje1<-(mujeres*100/total);
	porcentaje2<-(hombres*100/total);
	
	Escribir "en el grupo de estudiantes el ", porcentaje2," son hombres y el ", porcentaje1," son mujeres";
	
	
FinProceso
