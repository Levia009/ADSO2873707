Proceso ejercicio_Vl
	
	Definir metros,pies,pulgadas Como Real;
	
	Escribir "cuantos metros se encuentra su destino";
	Leer metros;
	
	pies<-( 3.28*metros );
	pulgadas<-( 39.37*metros );
	
	Escribir "su destino en pies se encuentra a ", pies, " y en pulgadas se encuentra a ", pulgadas;
	
FinProceso
